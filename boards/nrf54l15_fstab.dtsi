fstab {
    compatible = "zephyr,fstab";
    lfs1: lfs1 {
        compatible = "zephyr,fstab,littlefs";
        read-size = <16>;
        prog-size = <16>;
        cache-size = <64>;
        lookahead-size = <32>;
        block-cycles = <512>;
        partition = <&storage_partition>;
        mount-point = "/lfs1";
        automount;
    };
};
